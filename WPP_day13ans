const vowel = ["a", "e", "i", "o", "u"]

function countTheVowel(str) {
    let count = 0;

    for (let letter of str.toLowerCase()) {
        if (vowel.includes(letter)) {
            count++;
        }
    }
    return count
}

const string = prompt('Enter a string: ');

const result = countTheVowel(string);

console.log(result);
